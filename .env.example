# Telegram
TELEGRAM_BOT_TOKEN=replace_with_your_bot_token
TELEGRAM_CHAT_ID=replace_with_your_chat_id

# YouTube OAuth2 access token with youtube.upload scope
YOUTUBE_ACCESS_TOKEN=replace_with_access_token
YOUTUBE_CATEGORY_ID=22
YOUTUBE_PRIVACY=public

# TikTok Content Posting API access token
TIKTOK_ACCESS_TOKEN=replace_with_access_token

# If TikTok uses PULL_FROM_URL, provide a public URL for latest generated video
PUBLIC_VIDEO_URL=https://your-domain.com/path/to/video.mp4
